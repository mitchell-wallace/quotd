    "@capacitor/share": "^7.0.2",
    "@fontsource/architects-daughter": "^5.1.1",
    "@fontsource/covered-by-your-grace": "^5.1.1",
    "@fontsource/exo-2": "^5.2.5",
    "@fontsource/fredericka-the-great": "^5.1.1",
    "@fontsource/inconsolata": "^5.2.5",
    "@fontsource/josefin-slab": "^5.1.1",
    "@fontsource/lilita-one": "^5.2.5",
    "@fontsource/love-ya-like-a-sister": "^5.1.1",
    "@fontsource/maiden-orange": "^5.2.5",
    "@fontsource/nothing-you-could-do": "^5.1.1",
    "@fontsource/raleway": "^5.2.5",
    "@fontsource/roboto-slab": "^5.2.5",
    "@fontsource/smooch-sans": "^5.2.5",
    "@fontsource/syne-mono": "^5.1.1",
    "@fontsource/walter-turncoat": "^5.1.1",
    "@tabler/icons-vue": "^3.29.0",
    "@tailwindcss/vite": "^4.1.13",
    "html-to-image": "^1.11.13",
    "pinia": "^2.3.0",
    "tailwindcss": "^4.1.13",
    "unsplash-js": "^7.0.19",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
