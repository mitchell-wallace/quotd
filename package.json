{
  "name": "quotd",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test:e2e": "npm run build && playwright test",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "cap:sync": "cap sync",
    "cap:sync:ios": "cap sync ios",
    "cap:sync:android": "cap sync android --target Pixel_8_API_35",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android --target Pixel_8_API_35",
    "cap:run:ios": "cap run ios",
    "cap:run:android": "cap run android --target Pixel_8_API_35",
    "build:mobile": "npm run build && cap sync"
  },
  "dependencies": {
